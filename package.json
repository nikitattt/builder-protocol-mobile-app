{
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build:backend": "pnpm --filter \"./apps/backend\" build",
    "start:backend": "pnpm --filter \"./apps/backend\" start",
    "dev:backend": "pnpm --filter \"./apps/backend\" dev",
    "start:mobile": "pnpm --filter \"./apps/mobileapp\" start",
    "codegen:mobile": "pnpm --filter \"./apps/mobileapp\" codegen",
    "dev:website": "pnpm --filter \"./apps/website\" dev"
  },
  "devDependencies": {
    "prettier": "^2.5.1"
  },
  "workspaces": [
    "apps/*"
  ]
}
