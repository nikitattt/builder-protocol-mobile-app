{
  "name": "builder-protocol-mobile-app",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build:backend": "bun --filter backend build",
